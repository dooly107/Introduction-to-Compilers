PROGRAM test
BEGIN

	INT i, j;
	FLOAT newapprox,approx,num;
	STRING eol := "\n";

	FUNCTION VOID main()
	BEGIN
	
--		num := 7.0;			
        READ(num);
		j := 1;
		approx := num;	
		i := 100;

		WHILE(i != 0)
			newapprox := 0.5*(approx + num/approx);
			approx := newapprox;
			i := i-1;
		ENDWHILE
		        
		WRITE(approx, eol);
	END
END