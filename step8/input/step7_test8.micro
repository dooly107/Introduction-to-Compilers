PROGRAM p
BEGIN

	INT a,b,k,g,p,u;

	FUNCTION VOID main()
	BEGIN
        READ(a,b);
        WHILE(a <= 120) 
            IF (b = 1)
                g := 1*a;
                WRITE (g);
            ELSE
                IF (b = 2)
                    p := 2*a;
                    WRITE (p);
                ELSE
                    IF (b = 3)
                        k := 3*a;
                        WRITE (k);
                    ELSE
                        IF (b = 4)
                            u := 4*a;
                            WRITE (u);
                        ENDIF
                    ENDIF    
                ENDIF        
            ENDIF
            b := b + 1;
            a := a + 2;
        ENDWHILE
        WRITE(a,b);
	END
END
